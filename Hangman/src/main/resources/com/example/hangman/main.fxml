<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Pane fx:id="mainPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: ghostwhite;">
         <children>
            <Label layoutX="223.0" layoutY="65.0" text="Hangman">
               <font>
                  <Font name="Arial Black" size="49.0" />
               </font>
            </Label>
            <VBox layoutX="274.0" layoutY="231.0" spacing="20.0">
               <children>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClick" prefHeight="30.0" prefWidth="152.0" text="Start!">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="instructionButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="151.0" text="Instrukcja">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="editButton" mnemonicParsing="false" text="Edytuj istniejącą bazę">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="endButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="152.0" text="Zakończ">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</GridPane>
